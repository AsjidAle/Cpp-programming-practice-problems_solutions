#include<iostream>

#define size 10

void getter(int* arr){
    for(int i=0;i<10;i++){
        std::cout<<*(arr+i);
    }
}

void setter(int*arr){
    for(int i=0;i<10;i++){
        std::cin>>arr[i];
    }
}


int main(){
    int *arr=new int[size];//array creating of size "size"
    setter(arr);
    getter(arr);
    delete arr;// deallocating or free the arr pointer
    return 0;
}
















































